failure.failed: failure.cpp cxxanyargs.o
	$(Q)$(RUBY) -rfileutils \
	  -e "t = ARGV.shift" \
	  -e "err = IO.popen(ARGV, err:[:child, :out], &:read)" \
	  -e "abort err unless /rb_define_method/ =~ err" \
	  -e "FileUtils.touch(t)" $@ $(MAKE) failure.o

