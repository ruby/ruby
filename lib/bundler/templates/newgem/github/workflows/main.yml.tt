name: Ruby

on: [push,pull_request]

jobs:
  build:
    runs-on: ubuntu-latest
    steps:
    - uses: actions/checkout@v2
    - name: Set up Ruby
      uses: ruby/setup-ruby@v1
      with:
        ruby-version: <%= RUBY_VERSION %>
        bundler-cache: true
    - name: Run the default task
      run: bundle exec rake
