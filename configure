  setjmp) :
     setjmp_prefix= ;; #(
  setjmpex) :
     setjmp_prefix= setjmp_suffix=ex ;; #(
  '') :
     unset setjmp_prefix ;; #(
  *) :
       as_fn_error $? "invalid setjmp type: $withval" "$LINENO" 5 ;;
esac
