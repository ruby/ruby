module RubyVM::RJIT # :nodoc: all
  Instruction = Data.define(:name, :bin, :len)

  INSNS = {
% RubyVM::Instructions.each_with_index do |insn, i|
    <%= i %> => Instruction.new(
      name: :<%= insn.name %>,
      bin: <%= i %>, # BIN(<%= insn.name %>)
      len: <%= insn.width %>, # insn_len
    ),
% end
  }
end
